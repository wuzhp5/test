<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<!-- 条码功能，扩展，库存出库单据中扩展增加一个按钮：置为已回传 -->
	<bean id= "setBBackActionCardBean" class="nc.ui.pubapp.plugin.action.InsertActionInfo" >
               <property name= "pos" value ="after"></property>
               <property name= "actionType" value ="notedit"></property>
               <property name= "actionContainer" ref ="actionsOfCard"></property>
               <property name= "target" ref ="printMngAction"></property>
               <property name= "action" ref ="setBBackAction"></property>
    </bean>
	<bean id="setBBackAction" class="nc.ui.ic.m4c.action.plugin.SetBBackAction">
			<property name="editorModel" ref="icBizEditorModel" />
	</bean>
</beans>
